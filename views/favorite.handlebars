<h2>Favorites Page</h2>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">_id</th>
            <th scope="col">Title</th>
            <th scope="col">Director</th>
            <th scope="col">Rating</th>
        </tr>
    </thead>
    <tbody>
        {{#each movie}}
        <tr>
            <td>{{this._id}}</td>
            <td>{{this.title}}</td>
            <td>{{this.director}}</td>
            <td>{{this.rating}}/10</td>
            <td>
                <button class="movie-unfavorite" data-id={{this._id}}>Unfavorite Movie</button>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>